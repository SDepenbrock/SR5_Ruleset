<root>
	<windowclass name="charsheet_lines">
		<sheetdata>
			<frame_char name="linesframe">
				<bounds>15,10,-29,-5</bounds>
			</frame_char>
			<label_frametop>
				<anchored to="linesframe" />
				<static>Lines</static>
			</label_frametop>
			<stringu name="lines">
				<anchored to="linesframe">
					<top offset="30" />
					<left offset="15" />
					<right offset="-20" />
					<bottom offset="-15" />
				</anchored>
				<multilinespacing>20</multilinespacing>
			</stringu>
			<scrollbar_list>
				<anchored to="lines" />
				<target>lines</target>
			</scrollbar_list>
		</sheetdata>
	</windowclass>
</root>
